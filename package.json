{
  "name": "mapbox-to-ol-style",
  "version": "2.5.0",
  "description": "Utilities for creating OpenLayers style functions from Mapbox Style objects",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/boundlessgeo/mapbox-to-ol-style.git"
  },
  "bugs": {
    "url": "https://github.com/boundlessgeo/mapbox-to-ol-style/issues"
  },
  "keywords": [
    "openlayers",
    "mapbox"
  ],
  "author": "Andreas Hocevar <andreas.hocevar@gmail.com>",
  "contributors": [
    {
      "name": "Attila BerÃ©nyi",
      "email": "aberenyi@gislab.hu"
    },
    {
      "name": "Bart van den Eijnden",
      "email": "bartvde@boundlessgeo.com"
    },
    {
      "name": "Dan 'Ducky' Little",
      "email": "danlittle@yahoo.com"
    },
    {
      "name": "Yancy Matherne",
      "email": "ymatherne@boundlessgeo.com"
    },
    {
      "name": "Christian Mayer",
      "email": "chris@meggsimum.de"
    },
    {
      "name": "Antti Risteli",
      "email": "antti@kiinnost.us"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepublish": "npm run doc && mkdirp dist && browserify -g [ babelify --plugins [ add-module-exports transform-es2015-modules-commonjs ] ] -p [ standalonify --name mb2olstyle --deps [ null --ol/style/style ol.style.Style --ol/style/fill ol.style.Fill --ol/style/stroke ol.style.Stroke --ol/style/circle ol.style.Circle --ol/style/icon ol.style.Icon --ol/style/text ol.style.Text ] ] index.js > dist/mb2olstyle.js",
    "doc": "documentation readme -s API index.js",
    "pretest": "eslint src & npm run test-bundle",
    "test": "phantomjs --local-to-remote-url-access=true ./node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js test/index.html spec '{\"useColors\":true, \"ignoreResourceErrors\": true}'",
    "test-bundle": "browserify -g [ babelify --plugins [ transform-es2015-modules-commonjs ] ] test/test.js > test/bundle.js"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^9.0.0",
    "mapbox-to-css-font": "^2.0.3",
    "ol": "^4.2.0"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "browserify": "^14.4.0",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.18.0",
    "eslint-config-openlayers": "^7.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "mocha-phantomjs-core": "^2.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "should": "^11.2.1",
    "standalonify": "^0.1.3"
  }
}
